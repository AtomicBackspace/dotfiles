{
  "experimental": {
    "enableAgents": true,
    "plan": true
  },
  "general": {
    "defaultApprovalMode": "default",
    "preferredEditor": "neovim"
  },
  "hooks": {
    "AfterAgent": [
      {
        "hooks": [
          {
            "command": "/usr/local/bin/gatekeeper",
            "name": "gatekeeper",
            "type": "command"
          }
        ]
      }
    ]
  },
  "security": {
    "auth": {
      "selectedType": "gemini-api-key"
    },
    "environmentVariableRedaction": {
      "enabled": true
    }
  },
  "tools": {
    "disableLLMCorrection": true
  }
}